}
.contain{
  padding-bottom: 120rpx;
}
.top-contain,.tips-item{
  background-color: #fff;
  border-bottom: 4rpx solid #eee;
  box-sizing: border-box;
  padding: 30rpx 40rpx;
}
.wxParse-p{
  font-size: 28rpx;
  color: #999;
