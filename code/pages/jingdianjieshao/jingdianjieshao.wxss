/*横向排列的菜单*/
.nav-title {
  position: fixed;
  width: 100%;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  background-color: #fff;
  color: #666;
  font-size: 32rpx;
  z-index: 1000;
}
.nav-item {
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  padding: 0 20rpx;
}