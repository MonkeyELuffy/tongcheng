}

/*收货地址信息*/
.address {
  width: 100%;
  position: relative;
  background-color: #fff;
  box-sizing: border-box;
  border-top: 2rpx solid #eee;
  margin-bottom: 4rpx;
}

.address .address-message {
  flex: 1 1;
  padding: 20rpx 0rpx;
  width: 80%;
  padding-left: 30rpx;
  font-size: 30rpx;
  line-height: 50rpx;
  color: #333;
}
.address image {
  width: 40rpx;
  height: 50rpx;
  position: absolute;
  right: 10rpx;
  top: 50%;
  margin-top: -45rpx;
  padding: 20rpx;
}

.xiaoziti{
  font-size: 26rpx;
  color: #737373;
}

/*信息  */
.xinxi{
  width: 100%;
  font-size: 28rpx;
  background-color: #fff;
  padding: 20rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #eee;
}

/*配送方式  */
.zonge {
  position: relative;
  width: 100%;
  font-size: 28rpx;
  background-color: #fff;
