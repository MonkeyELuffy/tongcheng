/*清除浮动*/
.clear {
  width: 20rpx;
  height: 100rpx;
}

page {
  background: #f2ebe3;
}

.cart {
  padding: 200rpx 0 0 0;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  color: #999;
}

.cart image {
  width: 132rpx;
  height: 132rpx;
  margin-bottom: 40rpx;
}

.baoyou {
  font-size: 36rpx;
  color: #db2929;
  padding: 2rpx;
}

.goods {
  position: relative;
  background: #fff;
  padding: 20rpx 20rpx 0 0;
  display: flex;
  border-bottom: 2rpx solid #eee;
}

.goods image {
  width: 160rpx;
  height: 160rpx;
}

.goods .good-cont {
  display: flex;
  flex: 1;
  flex-direction: column;
  color: #555;
  font-size: 28rpx;
  padding: 10rpx;
  height: 160rpx;
}

.goods .good-cont .good-price {
  display: flex;
  flex: 1;
  justify-content: space-between;
  align-items: center;
}

.goods .good-cont .good-price .price {
  font-size: 32rpx;
  color: #ec5151;
}

.goods .good-cont .good-price .btn-box {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
  .goods .good-cont .good-price .btn-box .delete {
    position: absolute;
    right: 40rpx;
    top: 16rpx;
  }
.goods .good-cont .good-price .btn-box .delete image {
  width: 30rpx;
  height: 30rpx;
  border: 0;
  margin: 0;
}



.goods .good-cont .good-price .btn {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.goods .good-cont .good-price .btn input {
  margin: 0;
  width: 80rpx;
  text-align: center;
  font-size: 32rpx;
  height: 40rpx;
}

.goods .good-cont .good-price .btn button {
  margin: 0;
  width: 40rpx;
  height: 40rpx;
  border-radius: 60rpx;
  box-sizing: border-box;
  border: 2rpx solid #999;
  padding: 0;
  line-height: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
}

.total {
  height: 80rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  position: fixed;
  bottom: 0;
  width: 100%;
  z-index: 10;
  background-color: #fff;
  border: 0;
  margin: 0;
}

.total .total_text {
  display: flex;
  color: #333;
  height: 100%;
  line-height:80rpx;
  margin-left: 20rpx;
  font-size: 30rpx;
  font-weight: bold;
}

.total .total_text text {
  color: #ec5151;
}

.total .total_js {
  color: #fff;
  background: #ec5151;
  margin: 0;
   height: 100%; 
  border-radius: 0;
  line-height: 80rpx;
  width: 100%;
  font-size: 30rpx;
}
/*优惠金额*/
.youhui-total {
