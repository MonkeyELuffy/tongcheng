    flex-direction: column;
    margin: 40rpx 0 20rpx 20rpx;
  }
  .order-text {
    width: 100%;
    height: 80rpx;
    font-size: 24rpx;
    color: #666;
  }
  .order-price {
    display: flex;
    justify-content: space-between;
    box-sizing: border-box;
    padding: 30rpx;
    color: rgb(250,63,6);
    font-size: 28rpx;
		margin-bottom: 8rpx;
  }
  .order-num{
    font-size: 24rpx;
    color: #333;
  }
  .order-title{
    font-size: 32rpx;
    color: #333;
