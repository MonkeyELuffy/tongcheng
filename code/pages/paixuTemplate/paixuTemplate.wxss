/*排序  */
.tab{
  display: flex;
  align-items: center;
  background-color: #fff;
  border-bottom: 4rpx solid #eee;
  border-top: 4rpx solid #eee;
  color: #333;
}
.tab-item{
  width: 33%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  padding: 30rpx 0;
}
.tab-up-down{
  width: 22rpx;
  height: 38rpx;
  margin-left: 20rpx;
}