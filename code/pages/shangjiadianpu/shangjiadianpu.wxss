}
.product-img{
  width: 130rpx;
  height: 130rpx;
  margin-right: 24rpx;
}
.product-contain{
  flex: 1 1 ;
}
.product-name{
  font-size: 26rpx;
  color: #333;
  padding-bottom: 12rpx;
}
.product-title{
  font-size: 24rpx;
  color: #c2c2c2;
  line-height: 160%;
}
.product-price{
  font-size: 28rpx;
  color: #fa3f06;
  display: flex;
  align-items: flex-end;
  font-weight: 600;
}
.yuanjia{
  font-size: 22rpx;
  color: #999;
  padding-left: 16rpx;
  font-weight: 500;
  text-decoration:line-through;
}
.bottom-contain{
  height: 50rpx;
  position: relative;
  display: flex;
  align-items: center;
}
.zhekou-img{
  width: 66rpx;
  height: 40rpx;
}
.sale{
  font-size: 24rpx;
  color: #c2c2c2;
}
.choose-type{
  position: absolute;
  top: 0rpx;
  right: -20rpx;
  width: 114rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 50rpx;
  background-color: #fa3f06;
  color: #fff;
  font-size: 24rpx;
  border-radius: 6rpx;
}

  /*规格选择弹窗样式  */
  .mengban{
    width: 100%;
    height: 100vh;
    position: fixed;
    left: 0rpx;
    top: 0rpx;
    background-color: #666;
    opacity: 0.6;
    z-index: 1;
    box-sizing: border-box;
  }
  .type-contain{
    box-sizing: border-box;
    z-index: 11;
    width: 550rpx;
    min-height: 600rpx;
    position: absolute;
    left: 50%;
    margin-left: -275rpx;
    top: 200rpx;
    background-color: #fff;
    border-radius: 10rpx;
    overflow: hidden;
  }
  .type-contain-2{
    box-sizing: border-box;
    position: relative;
    width: 550rpx;
    min-height: 600rpx;
  }
  .close{
    position: absolute;
    right: 16rpx;
    top: 16rpx;
  }
  .type-contain image{
    width: 50rpx;
    height: 50rpx;
    border-radius: 50%;
  }
  .type-title{
    width: 100%;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 40rpx 0;
    font-size: 32rpx;
    color: #333;
  }
  .type-name{
    width: 100%;
    box-sizing: border-box;
    padding: 0 30rpx 20rpx ;
    font-size: 30rpx;
    color: #333;
  }
  .type-list{
    width: 100%;
    box-sizing: border-box;
    display: flex;
    padding: 0 30rpx;
    flex-wrap: wrap;
  }
  .type-item{
    box-sizing: border-box;
    padding: 14rpx 26rpx;
    border-radius: 10rpx;
    border: 2rpx solid rgb(0,188,213);
    color: #757575;
    font-size: 28rpx;
    margin: 0 30rpx 20rpx 0;
    text-align: center;
  }
  .count-contain{
    width: 100%;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    padding: 30rpx;
    justify-content: space-between;
  }
  .num{
    font-size: 30rpx;
    color: #333;
  }
  .add-sub{
    box-sizing: border-box;
    display: flex;
    align-items: center;
  }
  .clear-img,.car-top-contain image{
    width:30rpx;
    height:40rpx;
    margin:0 20rpx;
  }
  .count{
    width: 60rpx;
    padding: 0 20rpx;
    box-sizing: border-box;
    font-size: 30rpx;
    color: #333;
  }
  .add-to-car-contain{
    -ms-flex-item-align: end;
    width: 100%;
    height: 100rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    justify-content: space-between;
    background-color: rgba(0,188,213,0.6);
    padding-left: 30rpx;
    position: absolute;
    bottom: -2rpx;
  }
  .total-price{
    font-size: 30rpx;
    color: #fa3f06;
    font-weight: 600;
  }
  .add-to-car{
    width: 230rpx;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: rgb(0,188,213);
    color: #fff;
    font-size: 30rpx;
  }

/*评价样式  */
.comment-contain{
  width: 100%;
  position: relative;
}
.diancan-left-scroll-view,.comment-scroll-view{
  box-sizing: border-box;
  padding-bottom: 600rpx;
}
.shop-score{
  width: 100%;
  text-align: center;
  box-sizing: border-box;
  padding: 40rpx;
  font-size: 30rpx;
