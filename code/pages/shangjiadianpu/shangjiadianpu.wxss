/**index.wxss**/
.contain{
  width: 100%;
  position: relative;
  box-sizing: border-box;
  padding: 0;
  margin: 0;
  overflow: hidden;
  border-top: 10rpx solid #eee;
}
.choose-addr{
  margin: 0 20rpx 20rpx 20rpx;
  color: #757575;
  background-color: #eee;
  height: 70rpx;
  border-radius: 35rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  font-size: 28rpx;
}
.choose-addr image{
  width: 32rpx;
  height: 44rpx;
  margin: 0 30rpx 0 50rpx;
}

.shop-contain{
  display: flex;
  box-sizing: border-box;
  width: 100%;
  padding: 40rpx 20rpx;
  background-color: #fff;
  position: relative;
  align-items: center;
}
.shop-contain image{
  width: 180rpx;
  height: 180rpx;
  margin-right: 30rpx;
  border-radius: 6rpx;
}
.star{
  float: right;
  width: 50rpx;
  height: 50rpx;
}
.shop-name{
  color: #333;
  font-size: 30rpx;
  padding-bottom: 10rpx;
  font-weight: 600;
}
.shop-title{
  color: #757575;
  font-size: 24rpx;
}
.look-shop-detail{
  position: absolute;
  right: 20rpx;
  top: 0rpx;
  width: 120rpx;
  height: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 6rpx;
  border: 2rpx solid #faa819;
  font-size: 22rpx;
  color: #faa819;
  box-sizing: border-box;
}
.line{
  margin: 0 20rpx;
  background-color: #eee;
  height: 2rpx;
  margin-top: 10rpx;
}
.huodong-contain{
  display: flex;
  align-items: center;
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx 30rpx;
  color: #fa3f06;
  font-size: 30rpx;
  border-bottom: 10rpx solid #eee;
}
.huodong-contain image{
  width: 54rpx;
  height: 54rpx;
  margin-right: 20rpx;
}
.nav-contain{
  width: 100%;
  height: 84rpx;
  font-size: 32rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  border-bottom: 2rpx solid #eee;
}
.nav-item{
  box-sizing: border-box;
  color: #333;
  border-bottom: 2rpx solid #faa819;
  padding: 0 10rpx;
  padding-bottom: 8rpx;
}
/*点餐底部按钮  */
.bottom-btn{
  box-sizing: border-box;
  position: fixed;
  z-index: 1;
  bottom: 0rpx;
  left: 0rpx;
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  background-color: #fff;
  padding-left: 150rpx;
}
.bottom-btn image{
  width: 100rpx;
  height: 100rpx;
  background-color: #fff;
  border-radius: 50%;
  box-sizing: border-box;
  padding: 20rpx;
  position: absolute;
  z-index: 2;
  left: 20rpx;
  bottom: 20rpx;
}
.car-count{
  position: absolute;
  left: 100rpx;
  top: -10rpx;
  background-color: #fa3f06;
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  color: #fff;
  font-size: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 3;
  box-shadow: 0 0 4rpx 2rpx rgba(250,200,25,0.5); 
}
.peisong{
  font-size: 22rpx;
  color: #757575;
  padding-top: 10rpx;
}
.pay{
  position: absolute;
  right: 0rpx;
  top: 0rpx;
  width: 230rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
  background-color: #fa3f06;
  color: #fff;
  font-size: 30rpx;
}

/*点餐样式  */
.diancan-contain{
