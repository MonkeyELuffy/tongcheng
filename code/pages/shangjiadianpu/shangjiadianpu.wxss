  align-items: center;
  justify-content: center;
}
.right-contain{
  width: 80%;
  position: relative;
}
.right-title{
  border-left: 4rpx solid #ddd;
  background-color: #fff;
  color: #666;
  font-size: 22rpx;
  box-sizing: border-box;
  padding: 16rpx;
  display: flex;
  align-items: center;
}

.product-item{
  box-sizing: border-box;
  position: relative;
  padding: 16rpx 24rpx;
