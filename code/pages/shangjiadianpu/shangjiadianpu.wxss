  font-weight: 600;
}
.score{
  font-size: 40rpx;
  color: #fa3f06;
}
.comment-item{
  width: 100%;
  box-sizing: border-box;
  padding: 30rpx 20rpx 0 20rpx;
  display: flex;
}
.comment-img{
  width: 70rpx;
  height: 70rpx;
  margin-right: 20rpx;
  border-radius: 50%;
}
.comment-contain{
  flex: 1 1;
  border-bottom: 2rpx solid #eee;
}
.name-time{
  display: flex;
  align-items: center;
